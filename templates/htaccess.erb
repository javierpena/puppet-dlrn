<% if @distro_branch == "master" %>
RedirectMatch "^/(.*)/current-passed-ci(/|$)(.*)" "http://buildlogs.centos.org/centos/7/cloud/x86_64/rdo-trunk-master-tested/$3" 
RedirectMatch "^/(.*)/current-tripleo(/|$)(.*)" "http://buildlogs.centos.org/centos/7/cloud/x86_64/rdo-trunk-master-tripleo/$3"
<% else %>
RedirectMatch "^/(.*)/current-passed-ci(/|$)(.*)" "http://buildlogs.centos.org/centos/7/cloud/x86_64/rdo-trunk-<%= @release %>-tested/$3"
<% end %>
